<?xml version="1.0" encoding="utf-8" ?>
<siteMap xmlns="http://schemas.microsoft.com/AspNet/SiteMap-File-1.0" enableLocalization="true">
  <siteMapNode roles="*">

    <!-- Sample No Role -->

    <siteMapNode url="~/Login.aspx" resourceKey="Logout" roles="*"></siteMapNode>

    <siteMapNode url="~/Menu.aspx" resourceKey="Menu" roles="*"></siteMapNode>

    <siteMapNode resourceKey ="Upload Item Data" roles ="*">

      <siteMapNode url="~/UploadItemData.aspx" resourceKey="UploadItemData" roles="*"></siteMapNode>

    </siteMapNode>
    
    <siteMapNode resourceKey="Dashboard" roles="*">

