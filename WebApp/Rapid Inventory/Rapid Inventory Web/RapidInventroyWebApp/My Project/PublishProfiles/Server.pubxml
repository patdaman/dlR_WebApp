<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://www.rapidphysicalcount.com</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>WEBINVSERV</MSDeployServiceURL>
    <DeployIisAppPath>rapidphysicalcount.com</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>patrick</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="dbConn" Order="1" Enabled="False">
          <Destination Path="Data Source=WEBINVSERV;Initial Catalog=RAPIDINVENTORY;Integrated Security=False;Persist Security Info=True;User ID=sa;Password=LocalRposAdmin!" Name="Data Source=WEBINVSERV;Initial Catalog=RAPIDINVENTORY;Integrated Security=False;Persist Security Info=True;User ID=sa;Password=LocalRposAdmin!;MultipleActiveResultSets=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=MIKE-LAPTOP;Initial Catalog=RAPIDINVENTORY;Integrated Security=False;User ID=dorin;Password=Family2008" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\dbConn_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=MIKE-LAPTOP;Initial Catalog=RAPIDINVENTORY;Integrated Security=False;User Id=dorin;Password=Family2008;MultipleActiveResultSets=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)dbConn-Web.config Connection String">
      <ParameterValue>Data Source=WEBINVSERV;Initial Catalog=RAPIDINVENTORY;Integrated Security=False;Persist Security Info=True;User ID=sa;Password=LocalRposAdmin!;MultipleActiveResultSets=True</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
